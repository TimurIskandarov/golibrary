{
  "consumes": [
    "application/json",
    "application/x-www-form-urlencoded",
    "multipart/form-data"
  ],
  "produces": [
    "application/json"
  ],
  "schemes": [
    "http",
    "https"
  ],
  "swagger": "2.0",
  "info": {
    "description": "Документация Golibrary API.",
    "title": "Golibrary API.",
    "version": "1.0.0"
  },
  "host": "localhost:8080",
  "basePath": "/",
  "paths": {
    "/library/author": {
      "post": {
        "description": "Добавить автора",
        "tags": [
          "author"
        ],
        "operationId": "AuthorAdd",
        "parameters": [
          {
            "name": "Author",
            "in": "body",
            "schema": {
              "$ref": "#/definitions/Author"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Автор успешно добавлен",
            "schema": {
              "$ref": "#/definitions/Author"
            }
          }
        }
      }
    },
    "/library/authors": {
      "get": {
        "description": "Список авторов",
        "tags": [
          "author"
        ],
        "operationId": "Author",
        "responses": {
          "200": {
            "description": "Cписок авторов успешно получен",
            "schema": {
              "$ref": "#/definitions/Author"
            }
          }
        }
      }
    },
    "/library/books": {
      "get": {
        "description": "Список книг",
        "tags": [
          "book"
        ],
        "operationId": "BooksList",
        "responses": {
          "200": {
            "description": "Список книг успешно получен",
            "schema": {
              "$ref": "#/definitions/Book"
            }
          }
        }
      }
    },
    "/library/popular-authors": {
      "get": {
        "description": "Список популярных авторов",
        "tags": [
          "author"
        ],
        "operationId": "AuthorsTop",
        "responses": {
          "200": {
            "description": "Cписок популярных авторов успешно получен",
            "schema": {
              "$ref": "#/definitions/Author"
            }
          }
        }
      }
    },
    "/library/return/{bookId}/{userId}": {
      "put": {
        "description": "Сдать книгу",
        "tags": [
          "book"
        ],
        "operationId": "BookReturn",
        "responses": {
          "200": {
            "description": "Книга успешно сдана",
            "schema": {
              "$ref": "#/definitions/Book"
            }
          }
        }
      }
    },
    "/library/take/{bookId}/{userId}": {
      "put": {
        "description": "Взять книгу",
        "tags": [
          "book"
        ],
        "operationId": "BookTake",
        "responses": {
          "200": {
            "description": "Книга успешно получена",
            "schema": {
              "$ref": "#/definitions/Book"
            }
          }
        }
      }
    },
    "/library/users": {
      "get": {
        "description": "Список читателей",
        "tags": [
          "user"
        ],
        "operationId": "UsersList",
        "responses": {
          "200": {
            "description": "Список читателей успешно получен",
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        }
      }
    }
  },
  "definitions": {
    "Author": {
      "type": "object",
      "properties": {
        "birthDate": {
          "description": "in:body",
          "type": "string",
          "x-go-name": "BirthDate"
        },
        "books": {
          "description": "in:body",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Book"
          },
          "x-go-name": "Books"
        },
        "id": {
          "description": "in:body",
          "type": "integer",
          "format": "int64",
          "x-go-name": "ID"
        },
        "name": {
          "description": "in:body",
          "type": "string",
          "x-go-name": "Name"
        }
      },
      "x-go-package": "golibrary/internal/model"
    },
    "AuthorAdd": {
      "type": "object",
      "x-go-package": "golibrary/docs"
    },
    "Authors": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Author"
      },
      "x-go-package": "golibrary/docs"
    },
    "Book": {
      "type": "object",
      "properties": {
        "authorId": {
          "type": "integer",
          "format": "int64",
          "x-go-name": "AuthorID"
        },
        "id": {
          "description": "in:body",
          "type": "integer",
          "format": "int64",
          "x-go-name": "ID"
        },
        "title": {
          "type": "string",
          "x-go-name": "Title"
        },
        "userId": {
          "type": "integer",
          "format": "int64",
          "x-go-name": "UserID"
        }
      },
      "x-go-name": "BookReturn",
      "x-go-package": "golibrary/internal/model"
    },
    "Books": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/Book"
      },
      "x-go-package": "golibrary/docs"
    },
    "User": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "x-go-name": "Email"
        },
        "id": {
          "description": "in:body",
          "type": "integer",
          "format": "int64",
          "x-go-name": "ID"
        },
        "name": {
          "type": "string",
          "x-go-name": "Name"
        }
      },
      "x-go-name": "UsersList",
      "x-go-package": "golibrary/docs"
    }
  },
  "securityDefinitions": {
    "api_key": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  },
  "security": [
    {
      "api_key": []
    }
  ]
}